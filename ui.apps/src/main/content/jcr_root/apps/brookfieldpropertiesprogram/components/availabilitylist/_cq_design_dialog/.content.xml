<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="AvailabilityList Configuration"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Text"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <field3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        jcr:title="Configuration">                                                   
                                        <items
                                            jcr:primaryType="nt:unstructured">                                                     
                                            <lblTabTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lblTabTitle"
                                                name="./lblTabTitle"/>  
                                            <lblTabTitleInteract
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lblTabTitleInteract"
                                                name="./lblTabTitleInteract"/>  
                                            <lblMoveInDate
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lblMoveInDate"
                                                name="./lblMoveInDate"/>   
                                            <lblHomeType
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lbHomeType"
                                                name="./lblHomeType"/>   
                                            <lbStudio
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lbStudio"
                                                name="./lblStudio"/>
                                            <lb1Bd
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lb1Bd"
                                                name="./lbl1Bd"/>
                                            <lb2Bd
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lb2Bd"
                                                name="./lbl2Bd"/>
                                            <lblPH
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lbPH"
                                                name="./lblPH"/>
                                            <lblTH
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lbTH"
                                                name="./lblTH"/>
                                            <lblMinRent
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="lblMinRent"
                                                    name="./lblMinRent"/>
                                            <lblMaxRent
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lblMaxRent"
                                                name="./lblMaxRent"/>                                                                                          
                                            <lblFloorPlan
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lblFloorPlan"
                                                name="./lblFloorPlan"/>
                                            <lblBedBath
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lblBedBath"
                                                name="./lblBedBath"/>
                                            <lblSize
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lblSize"
                                                name="./lblSize"/>
                                            <lblRent
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lblRent"
                                                name="./lblRent"/>
                                            <lblAvailability
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lblAvailability"
                                                name="./lblAvailability"/>
                                            <lblViewApartment
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lblViewApartment"
                                                name="./lblViewApartment"/>     
                                             <lblViewApartment
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="lblViewResults"
                                                name="./lblViewResults"/>                                                                                                                                                                                                                                                                                                             
                                        </items>
                                    </field3>                                                                      
                                </items>
                            </columns>
                        </items>
                    </general>  
                    <Configuration
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <field3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        jcr:title="Configuration">                                                   
                                        <items
                                            jcr:primaryType="nt:unstructured">                                                     
                                            <rentMin
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="rentMin"
                                                required="true"
                                                name="./rentMin"/>   
                                            <rentMinTypeHint
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                ignoreData="{Boolean}true"
                                                name="./rentMin@TypeHint"
                                                value="Long"/>  
                                            <rentIncrementBy
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="rentIncrementBy"
                                                required="true"
                                                name="./rentIncrementBy"/>   
                                            <rentIncrementByTypeHint
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                ignoreData="{Boolean}true"
                                                name="./rentIncrementBy@TypeHint"
                                                value="Long"/>  
                                            <rentMax
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="rentMax"
                                                required="true"
                                                name="./rentMax"/>   
                                            <rentMaxTypeHint
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                ignoreData="{Boolean}true"
                                                name="./rentMax@TypeHint"
                                                value="Long"/>                                                                                                                                                                                                                                                                                                                                                                                                                                        
                                        </items>
                                    </field3>                                                                      
                                </items>
                            </columns>
                        </items>
                    </Configuration>     
                    <Links
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Links"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <field3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        jcr:title="Configuration">                                                   
                                        <items
                                            jcr:primaryType="nt:unstructured">                                                     
                                            <viewApartmentLink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="viewApartmentLink"
                                                name="./viewApartmentLink"/>                                                                                                                                                                                                                                                                                                                                                                   
                                        </items>
                                    </field3>                                                                      
                                </items>
                            </columns>
                        </items>
                    </Links>                           
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
