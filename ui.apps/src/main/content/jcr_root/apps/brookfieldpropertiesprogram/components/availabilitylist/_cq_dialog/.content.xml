<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Availability List Configuration"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Engrain Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <field3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        jcr:title="Configuration">                                                   
                                        <items
                                            jcr:primaryType="nt:unstructured">                                                     
                                            <btnType
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Hide UI Elements"
                                                multiple="{Boolean}true"
                                                name="./hideUI">
                                                <items jcr:primaryType="nt:unstructured">                                               
                                                    <filters jcr:primaryType="nt:unstructured" text="Filters" value="filters"/>
                                                    <unitList jcr:primaryType="nt:unstructured" text="Unit List" value="unitList"/>
                                                    <floorSelection jcr:primaryType="nt:unstructured" text="Floor Selection" value="floorSelection"/>
                                                    <unitToolTip jcr:primaryType="nt:unstructured" text="Unit ToolTip" value="unitToolTip"/>
                                                    <unitDetails jcr:primaryType="nt:unstructured" text="Unit Details" value="unitDetails"/>
                                                </items>
                                            </btnType>                                                                                                                                                                                                                                                   
                                        </items>
                                    </field3>                                                                      
                                </items>
                            </columns>
                        </items>
                    </general>      
                    <Configuration
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Configuration"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <field3
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                        jcr:title="Configuration">                                                   
                                        <items
                                            jcr:primaryType="nt:unstructured">                                                     
                                            <linkToAvailability
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="Check to hide the Gallery"
                                                name="./linkToAvailability"
                                                text="Link To Availability (do not run search)"
                                                checked="{Boolean}false"
                                                uncheckedValue="false"
                                                value="true"/>                                                                                                                                                                                                                                                                                                                                                                                                                                     
                                        </items>
                                    </field3>                                                                      
                                </items>
                            </columns>
                        </items>
                    </Configuration>                                 
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
